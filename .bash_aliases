alias mage_cc='zf clear mage-core-cache'
alias mage_config_get='zf show mage-core-config'
alias mage_config_set='zf set mage-core-config'
# turn template hints on/off
alias mage_hints_on='zf set mage-core-config dev/debug/template_hints stores --value 1'
alias mage_hints_off='zf set mage-core-config dev/debug/template_hints stores --value 0'
